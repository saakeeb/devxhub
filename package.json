{
  "name": "devxhub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && set \"GENERATE_SOURCEMAP=false\" && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write './**/*.{js,jsx,ts,tsx,css}'",
    "start:local": "cross-env env-cmd -f environments/.env.local vite",
    "start:development": "env-cmd -f environments/.env.development vite",
    "build:development": "env-cmd -f environments/.env.development vite && vite export -o build",
    "build:local": "env-cmd -f environments/.env.local vite && next export -o build",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@types/react-devtools-inline": "^4.24.3",
    "@types/video-react": "^0.15.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "gsap": "^3.12.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-devtools-inline": "^4.27.8",
    "react-dom": "^18.2.0",
    "react-gsap": "^3.2.1",
    "react-icons": "^4.9.0",
    "react-intersection-observer": "^9.4.4",
    "react-router-dom": "^6.12.0",
    "video-react": "^0.16.0"
  },
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "tailwindcss-css-filters": "^2.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
